{
  "name": "projectrebel/laravel-active-campaign",
  "description": "A fluent wrapper around version 3 of the Active Campaign API - extended from ProjectRebel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tim Wiffen",
      "email": "tim@reclaro.com"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^7.0.1"
  },
  "autoload": {
    "psr-4": {
      "ProjectRebel\\ActiveCampaign\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ProjectRebel\\ActiveCampaign\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ProjectRebel\\ActiveCampaign\\ActiveCampaignServiceProvider"
      ],
      "aliases": {
        "ActiveCampaign": "ProjectRebel\\ActiveCampaign\\Facades\\ActiveCampaign"
      }
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.4"
  }
}
